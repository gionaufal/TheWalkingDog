<%= simple_form_for @walker do |f| %>
  <%= f.input :name %>
  <%= f.input :age, as: :integer %>
  <%= f.input :address %>
  <%= f.input :cpf %>
  <%= f.input :bio %>
  <%= f.input :experience %>
  <%= f.input :price, as: :float %>
  <%= f.input :email, as: :email %>
  <%= f.input :phone, as: :tel %>
  <%= f.input :region %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>
  <%= f.submit 'Cadastrar' %>
<% end %>
