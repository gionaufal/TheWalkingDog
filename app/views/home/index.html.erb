<section id='about'>
  <p>
    Encontre pessoas especializadas para passear com seu melhor amigo próximas a você
  </p>
</section>

<section class='walkers'>
  <%= @walkers.each do |walker| %>
    <%= link_to walker do %>
    <div class="walker">
      <p><%= walker.name %></p>
      <p><%= walker.region %></p>
    </div>
    <% end %>
  <% end %>

</section>
